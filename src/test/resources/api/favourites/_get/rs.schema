{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": { "type": "integer" },
      "image_id": { "type": "string" },
      "sub_id": { "type": ["string", "null"] },
      "created_at": { "type": "string" },
      "image": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "url": { "type": "string", "pattern": "^https?://" }
        },
        "required": ["id", "url"]
      }
    },
    "required": ["id", "image_id", "created_at"]
  }
}
