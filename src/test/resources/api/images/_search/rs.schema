{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": { "type": "string" },
      "url": { "type": "string", "pattern": "^https?://" },
      "width": { "type": ["integer", "null"] },
      "height": { "type": ["integer", "null"] },
      "breeds": {
        "type": "array",
        "items": { "type": "object" }
      }
    },
    "required": ["id", "url"]
  }
}